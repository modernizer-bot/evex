{
  "rules": {
    "no-debugger": "off",
    "no-console": "off",
    "no-unused-vars": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "globals": {
    "atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "plugins": ["react"]
}
